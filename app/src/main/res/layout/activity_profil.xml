<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".views.ProfilActivity">

    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/experience" tools:layout_editor_absoluteY="79dp"
            tools:layout_editor_absoluteX="0dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/level"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/username" tools:layout_editor_absoluteY="36dp"
            tools:layout_editor_absoluteX="0dp"/>
    <lecho.lib.hellocharts.view.PieChartView
            android:id="@+id/chart"
            android:layout_width="214dp"
            android:layout_height="202dp" tools:layout_editor_absoluteY="55dp"
            tools:layout_editor_absoluteX="194dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>